        "eslint-plugin-react-hooks": "4.6.2",
        "eslint-plugin-react-refresh": "0.4.20",
        "eslint-plugin-storybook": "0.12.0",
        "eslint-plugin-tss-unused-classes": "^1.0.3",
        "postcss": "8.5.3",
        "prettier": "3.5.3",
        "storybook": "8.6.14",
        "tailwindcss": "3.4.17",
